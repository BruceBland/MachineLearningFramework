  # Use sampling tecnique to remove bias in results
  # First create an index
  ReturnDF$Index <- seq(1,nrow(ReturnDF))
  
  # Now subset the data
  Ups <- subset(ReturnDF,Real5 > 0)
  Ups$TempIndex <- seq(1,nrow(Ups))
  Downs <- subset(ReturnDF,Real5 < 0)
  Downs$TempIndex <- seq(1,nrow(Downs))
  Sames <- subset(ReturnDF,Real5 == 0)
  Sames$TempIndex <- seq(1,nrow(Sames))
  
  # Work out which subset is the smallest
  MinOfSamples <- min(nrow(Ups),nrow(Downs),nrow(Sames)) 
  
  # Sample the rest to get the same number of occurences
  UpsIndex <- sample(Ups$TempIndex,MinOfSamples,replace = FALSE,prob=NULL)
  DownsIndex <- sample(Downs$TempIndex,MinOfSamples,replace = FALSE,prob=NULL)
  SamesIndex <- sample(Sames$TempIndex,MinOfSamples,replace = FALSE,prob=NULL)
  
  # Now extract the samples
  Ups <- Ups[UpsIndex,]
  Downs <- Downs[DownsIndex,]
  Sames <- Sames[SamesIndex,]
  
  # Bind the data back together again
  ReturnDF <- rbind(Ups,Downs,Sames)
  
  # Sort the data back into order
  library(plyr)
  
  ReturnDF <- arrange(ReturnDF,Index)
 
  ReturnDF$Index <- NULL
  ReturnDF$TempIndex <- NULL
